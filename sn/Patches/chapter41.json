	{
		"LABEL": "0x17CB",
		"CMD": "TEXT2",
		"TYPE": -1,
		"STRING": "There were sounds of a flurry of activity――"
	},
	{
		"LABEL": "0x17E9",
		"CMD": "TEXT2",
		"TYPE": -1,
		"STRING": "Without warning, the door opened."
	},
	{
		"LABEL": "0x180D",
		"CMD": "TEXT2",
		"TYPE": -1,
		"STRING": "A blinding light shone onto the temporary night sky."
	},
	{
		"LABEL": "0x1896",
		"CMD": "TEXT2",
		"TYPE": -1,
		"STRING": "「They're here!」"
	},
	{
		"LABEL": "0x18B4",
		"CMD": "TEXT2",
		"TYPE": -1,
		"STRING": "「Survivors――」"
	},
	{
		"LABEL": "0x18D0",
		"CMD": "TEXT2",
		"TYPE": -1,
		"STRING": "「Two survivors confirmed!」"
	}
